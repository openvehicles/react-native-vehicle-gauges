{
  "name": "react-native-vehicle-gauges",
  "version": "1.0.7",
  "description": "A comprehensive suite of React Native components for displaying vehicle gauge clusters as seen in cars and motorcycles",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prepublishOnly": "npm run build",
    "web": "vite",
    "web:build": "vite build"
  },
  "keywords": [
    "react-native",
    "gauges",
    "speedometer",
    "vehicle",
    "dashboard",
    "automotive",
    "motorcycle",
    "car",
    "components",
    "ui"
  ],
  "author": "Mark Webb-Johnson <mark@openvehicles.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openvehicles/react-native-vehicle-gauges.git"
  },
  "bugs": {
    "url": "https://github.com/openvehicles/react-native-vehicle-gauges/issues"
  },
  "homepage": "https://github.com/openvehicles/react-native-vehicle-gauges#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0",
    "react-native-svg": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.1",
    "babel-loader": "^10.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "html-webpack-plugin": "^5.6.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.21.1",
    "typescript": "^5.1.0",
    "vite": "^7.1.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "files": [
    "dist/",
    "src/",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
